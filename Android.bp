cc_binary {
    name: "overlaydemo_vulkan",
    srcs: ["overlaydemo_vulkan.cpp"],

    shared_libs: [
        "libvulkan",
        "libui",
        "libgui",
        "libandroid",
        "libbinder",
        "libutils",
    ],

    cflags: [
        "-Wno-unused-parameter",
        "-Wno-unused-variable",
    ],

    host_supported: false,

    multilib: {
        lib32: {
            enabled: false,
        },
        lib64: {
            enabled: true,
        },
    },
}
