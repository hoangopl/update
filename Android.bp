cc_binary {
    name: "overlaydemo",
    srcs: ["overlaydemo.cpp"],

    shared_libs: [
        "libbinder",
        "libutils",
        "libcutils",
        "libgui",
        "libui",
        "libandroid",
    ],

    cflags: ["-Wno-unused-parameter"],

    host_supported: false,

    multilib: {
        lib32: {
            enabled: false,
        },
        lib64: {
            enabled: true,
        },
    },
}
